{
    "relayController": {
        "relays": {
            "$count": 6,
            "$w": "setRelay",
            "$type": "boolean"
        }
    },
    "pwmController": {
        "bankA": {
            "frequency": {
                "$w": "setPwmFrequencyA",
                "$type": "integer"
            }
        },
        "bankB": {
            "frequency": {
                "$w": "setPwmFrequencyB",
                "$type": "integer"
            }
        },
        "pwmCurrentMonitorUpdateRate": {
            "$w": "setPwmCurrentMonitorUpdateRate",
            "$type": "integer"
        },
        "faultThreshold": {
            "$w": "setPwmFaultThreshold",
            "$type": "integer"
        },
        "measuredCurrents": {
            "$count": 12,
            "$s": true,
            "$type": "integer"
        },
        "pwms": {
            "$count": 12,
            "activeConfiguration": {
                "$w": "sethiconf",
                "$type": "string"
            },
            "dutyCycle": {
                "$w": "setPwmDutyCycle",
                "$type": "integer"
            },
            "enabled": {
                "$w": "setPwmEnabled",
                "$type": "boolean"
            },
            "profile": {},
            "currentMonitor": {
                "faultDelay": {
                    "$w": "setPwmFaultDelay",
                    "$type": "integer"
                },
                "faulty": {
                    "$type": "boolean"
                },
                "enabled": {
                    "$w": "setPwmCurrentMonitorEnabled",
                    "$type": "boolean"
                }
            }
        }
    },
    "adcInputController": {
        "adcInputMonitorRate": {
            "$w": "setAdcMonitorUpdateRate",
            "$type": "integer"
        },
        "adcInputs": {
            "$count": 8,
            "enabled": {
                "$w": "setAdcEnabled",
                "$type": "boolean"
            }
        },
        "adcInputVoltages": {
            "$count": 8,
            "$s": true,
            "$type": "integer"
        }
    },
    "digitalInputController": {
        "digitalInputMonitorRate": {
            "$w": "setDiMonitorUpdateRate",
            "$type": "integer"
        },
        "digitalInputs": {
            "$count": 12,
            "$s": true,
            "$type": "boolean"
        }
    },
    "frequencyInputController": {
        "frequencyMonitorRate": {
            "$w": "setFrequencyMonitorRate",
            "$type": "integer"
        },
        "measuredFrequencies": {
            "$count": 4,
            "$s": true,
            "$type": "integer"
        },
        "frequencyInputs": {
            "$count": 4,
            "enabled": {
                "$w": "setFrequencyInputEnabled",
                "$type": "boolean"
            }
        }
    },
    "canDatabase": {
        "$type": "string"
    },
    "usb": {
        "txMessage": {
            "$w": "sendrawusb",
            "$type": "hex"
        },
        "rxMessage": {
            "$type": "hex"
        },
        "rxCount": {
            "$type": "integer"
        },
        "usbinfo": {
            "$type": "string"
        }
    },
    "version": {
        "$r": "getFirmwareVersion",
        "$type": "string"
    }
}