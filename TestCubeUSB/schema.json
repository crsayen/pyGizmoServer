{
    "relayController": {
        "relays": {
            "$count": 6,
            "$read": "getRelay",
            "$write": "setRelay",
            "$type": "boolean"
        }
    },
    "pwmController": {
        "bankA": {
            "frequency": {
                "$write": "setPwmFrequencyA",
                "$type": "integer"
            }
        },
        "bankB": {
            "frequency": {
                "$write": "setPwmFrequencyB",
                "$type": "integer"
            }
        },
        "pwmCurrentMonitorUpdateRate": {
            "$write": "setPwmCurrentMonitorUpdateRate",
            "$type": "integer"
        },
        "faultThreshold": {
            "$write": "setPwmFaultThreshold",
            "$type": "integer"
        },
        "measuredCurrents": {
            "$count": 12,
            "$read": "getActuatorCurrent",
            "$watchable": true,
            "$type": "integer"
        },
        "faults": {
            "$count": 12,
            "$watchable": true,
            "$type": "boolean"
        },
        "pwms": {
            "$count": 12,
            "activeConfiguration": {
                "$write": "sethiconf",
                "$type": "string"
            },
            "dutyCycle": {
                "$write": "setPwmDutyCycle",
                "$type": "integer"
            },
            "enabled": {
                "$write": "setPwmEnabled",
                "$type": "boolean"
            },
            "profile": {
                "start": {
                    "$write": "startPwmProfile",
                    "$type": "boolean"
                },
                "pause": {
                    "$write": "pausePwmProfile",
                    "$type": "boolean"
                },
                "stop": {
                    "$write": "stopPwmProfile",
                    "$type": "boolean"
                }
            }
        },
        "profiles": {
            "upLoadProfile": {
                "$write": "uploadPwmProfile",
                "$type": "string"
            },
            "startProfiles": {
                "$write": "startPwmProfiles",
                "$type": "hex"
            },
            "stopProfiles": {
                "$write": "stopPwmProfiles",
                "$type": "hex"
            },
            "pauseProfiles": {
                "$write": "pausePwmProfiles",
                "$type": "hex"
            },
            "dutyCycles": {
                "$count": 12,
                "$watchable": true,
                "$type": "integer"
            },
            "indecies": {
                "$count": 12,
                "$watchable": true,
                "$type": "integer"
            },
            "frequencies": {
                "$count": 2,
                "$watchable": true,
                "$type": "integer"
            },
            "enableUpdates": {
                "$type": "boolean",
                "$write": "enablePwmProfileUpdates"
            }
        }
    },
    "adcInputController": {
        "adcInputMonitorRate": {
            "$write": "setAdcMonitorUpdateRate",
            "$type": "integer"
        },
        "adcInputs": {
            "$count": 8,
            "enabled": {
                "$write": "setAdcEnabled",
                "$type": "boolean"
            }
        },
        "adcInputVoltages": {
            "$count": 8,
            "$watchable": true,
            "$read": "getAdcVoltage",
            "$type": "integer"
        }
    },
    "digitalInputController": {
        "digitalInputMonitorRate": {
            "$write": "setDiMonitorUpdateRate",
            "$type": "integer"
        },
        "digitalInputs": {
            "$count": 12,
            "$watchable": true,
            "$read": "getDI",
            "$type": "boolean"
        }
    },
    "frequencyInputController": {
        "frequencyMonitorRate": {
            "$write": "setFrequencyMonitorRate",
            "$type": "integer"
        },
        "measuredFrequencies": {
            "$count": 4,
            "$watchable": true,
            "$read": "getFrequency",
            "$type": "integer"
        },
        "frequencyInputs": {
            "$count": 4,
            "enabled": {
                "$write": "setFrequencyInputEnabled",
                "$type": "boolean"
            },
            "frequency": {"$read": "getFrequency", "$type": "integer"}
        }
    },
    "canDatabase": {
        "$type": "string"
    },
    "usb": {
        "txMessage": {
            "$write": "sendrawusb",
            "$type": "message"
        },
        "rxMessage": {
            "$type": "hex",
            "$watchable": true
        },
        "rxCount": {
            "$type": "integer",
            "$watchable": true
        },
        "usbinfo": {
            "$type": "string"
        }
    },
    "version": {
        "$read": "getFirmwareVersion",
        "$type": "string"
    },
    "heartbeat": {
        "$type": "integer",
        "$watchable": true
    }
}